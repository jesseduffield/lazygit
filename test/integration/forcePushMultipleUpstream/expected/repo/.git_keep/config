[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
	ignorecase = true
	precomposeunicode = true
[user]
	email = CI@example.com
	name = CI
[push]
	default = upstream
[remote "origin"]
	url = ../origin
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "other_branch"]
	remote = origin
	merge = refs/heads/other_branch
